{
  "name": "crassa-material-ui",
  "version": "1.0.0",
  "description": "CRASSA with Material-UI",
  "homepage2": "https://grovertb.github.io/crassa-material-ui/",
  "main": "index.js",
  "scripts": {
    "start": "crassa start",
    "build": "crassa build",
    "dev": "crassa dev",
    "lint": "crassa lint",
    "count": "crassa count-lines",
    "predeploy": "npm run build",
    "deploy": "gh-pages -b gh-pages -d build",
    "lint-staged": "lint-staged"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "crassa": {
    "displayName": "crassa-material-ui",
    "aliases": {
      "utils": "./src/utils",
      "lib": "./src/lib",
      "reducers": "./src/reducers",
      "sagas": "./src/sagas",
      "components": "./src/components",
      "containers": "./src/containers",
      "api": "./src/api",
      "sources": "./src/sources"
    }
  },
  "_moduleAliases": {
    "utils": "./src/utils",
    "lib": "./src/lib",
    "reducers": "./src/reducers",
    "sagas": "./src/sagas",
    "components": "./src/components",
    "containers": "./src/containers",
    "api": "./src/api",
    "sources": "./src/sources"
  },
  "jest": {
    "moduleNameMapper": {
      "utils": "<rootDir>/src/utils",
      "lib": "<rootDir>/src/lib",
      "reducers": "<rootDir>/src/reducers",
      "sagas": "<rootDir>/src/sagas",
      "components": "<rootDir>/src/components",
      "containers": "<rootDir>/src/containers",
      "api": "<rootDir>/src/api",
      "sources": "<rootDir>/src/sources"
    }
  },
  "dependencies": {
    "@material-ui/core": "3.5.1",
    "@material-ui/icons": "3.0.1",
    "crassa": "0.4.2",
    "extensible-duck": "1.4.0",
    "loadable-components": "2.2.3",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "react-helmet": "5.2.0",
    "react-hot-loader": "4.3.12",
    "react-jss": "8.6.1",
    "react-redux": "5.1.1",
    "react-router-dom": "4.3.1",
    "react-router-redux": "5.0.0-alpha.9",
    "redux": "4.0.1",
    "redux-logger": "3.0.6",
    "redux-saga": "0.16.2"
  },
  "devDependencies": {
    "gh-pages": "2.0.1",
    "redux-devtools": "3.4.2",
    "redux-devtools-dock-monitor": "1.1.3",
    "redux-devtools-log-monitor": "1.4.0"
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "{src,server}/**/*.js": [
      "eslint . --ext .js --fix",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
