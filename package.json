{
  "name": "{-- project-name --}",
  "version": "1.0.0",
  "description": "CRASSA with Material-UI",
  "main": "index.js",
  "scripts": {
    "start": "crassa start",
    "build": "crassa build",
    "dev": "crassa dev",
    "lint": "crassa lint",
    "count": "crassa count-lines",
    "predeploy": "npm run build",
    "deploy": "gh-pages -b gh-pages -d build",
    "lint-staged": "lint-staged"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "crassa": {
    "displayName": "{-- project-name --}"
  },
  "_moduleAliases": {
    "components": "./src/components",
    "containers": "./src/containers",
    "reducers": "./src/reducers",
    "sagas": "./src/sagas",
    "sources": "./src/sources",
    "utils": "./src/utils"
  },
  "jest": {
    "moduleNameMapper": {
      "utils": "<rootDir>/src/utils",
      "lib": "<rootDir>/src/lib",
      "reducers": "<rootDir>/src/reducers",
      "sagas": "<rootDir>/src/sagas",
      "components": "<rootDir>/src/components",
      "containers": "<rootDir>/src/containers",
      "api": "<rootDir>/src/api",
      "assets": "<rootDir>/src/assets"
    }
  },
  "dependencies": {
    "@loadable/component": "5.10.2",
    "@material-ui/core": "4.2.1",
    "@material-ui/icons": "4.2.1",
    "axios": "0.19.0",
    "axios-debug-log": "0.6.2",
    "connected-react-router": "6.5.2",
    "crassa": "git+https://github.com/grovertb/crassa.git",
    "extensible-duck": "1.5.0",
    "js-cookie": "2.2.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-hot-loader": "4.11.0",
    "react-redux": "7.1.0",
    "react-router-dom": "5.0.1",
    "redux": "4.0.4",
    "redux-logger": "3.0.6",
    "redux-saga": "1.0.5",
    "redux-wait-for-action": "0.0.8"
  },
  "devDependencies": {
    "eslint-plugin-sort-keys-fix": "1.0.1",
    "gh-pages": "2.0.1",
    "redux-devtools": "3.5.0",
    "redux-devtools-dock-monitor": "1.1.3",
    "redux-devtools-log-monitor": "1.4.0"
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "{src,server}/**/*.js": [
      "eslint . --ext .js --fix",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
